# Environment variables for apps/admin

# Node environment
NODE_ENV=development

# Clerk (authentication)
# Frontend publishable key (exposed to client)
VITE_CLERK_PUBLISHABLE_KEY=


# API base URL for admin app
VITE_ADMIN_API_BASE_URL=

# Optional: Force a specific actor/user id for debugging (sends x-actor-id/x-user-id)
# Use only in local dev; leave empty in production
VITE_DEBUG_ACTOR_ID=


