{
    "name": "@repo/seed",
    "version": "0.1.0",
    "description": "Database seeding package",
    "private": true,
    "type": "module",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "require": "./src/index.ts"
        }
    },
    "files": ["dist"],
    "scripts": {
        "lint": "biome check .",
        "format": "biome format --write .",
        "check": "biome check --write .",
        "clean": "rm -rf node_modules dist",
        "seed": "tsx ./src/cli.ts",
        "seed:required": "tsx ./src/seeds/required.seed.ts",
        "seed:example": "tsx ./src/seeds/example.seed.ts",
        "typecheck": "tsc --noEmit",
        "migrate:accommodation-prices": "tsx ./scripts/migrate-accommodation-prices.ts",
        "validate:accommodations": "tsx ./scripts/validate-fix-accommodations.ts"
    },
    "dependencies": {
        "@repo/config": "workspace:*",
        "@repo/db": "workspace:*",
        "@repo/logger": "workspace:*",
        "@repo/schemas": "workspace:*",
        "@repo/service-core": "workspace:*",
        "@repo/types": "workspace:*",
        "@repo/utils": "workspace:*",
        "dotenv": "^17.2.1",
        "pg": "^8.11.1",
        "zod": "^4.0.8"
    },
    "devDependencies": {
        "@repo/typescript-config": "workspace:*",
        "@types/node": "^22.15.3",
        "@types/pg": "^8.15.1",
        "tsup": "^8.4.0",
        "tsx": "^4.19.4",
        "typescript": "^5.8.3"
    }
}
