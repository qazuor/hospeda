{
    "name": "@repo/ai-image-generation",
    "version": "0.1.0",
    "description": "AI-powered image generation package with multi-provider support",
    "private": true,
    "type": "module",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "types": "./src/index.ts"
        },
        "./config": {
            "import": "./src/config/index.ts",
            "types": "./src/config/index.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "echo 'Dev mode uses tsconfig paths (no build)'",
        "lint": "biome check .",
        "format": "biome format --write .",
        "check": "biome check --write .",
        "clean": "rm -rf node_modules dist",
        "typecheck": "tsc --noEmit --project ./tsconfig.json",
        "test": "vitest run --passWithNoTests",
        "test:watch": "vitest",
        "test:file": "vitest run --passWithNoTests",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "replicate": "^0.25.0",
        "sharp": "^0.32.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@repo/typescript-config": "workspace:*",
        "@types/node": "^22.15.3",
        "@vitest/coverage-v8": "^3.1.3",
        "@vitest/ui": "^3.1.4",
        "dotenv": "^17.2.3",
        "tsup": "^8.4.0",
        "tsx": "^4.19.4",
        "typescript": "^5.8.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.1.3"
    }
}
